<div class='row'>
  <div class="col-xs-12 center ">
    <h1 class='formTitle'>Categories Listing</h1>
  </div>
</div>
<% @categories.each do |category| %>
  <ul class="listing">
    <div class="row">
      <div class="well col-md-4 col-md-offset-4">
        <li class="article-title">
          <%= link_to "#{category.name}", category_path(category) %>
        </li>
      </div>
    </div>
  </ul>
<% end %>